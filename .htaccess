<IfModule mod_rewrite.c>
	RewriteEngine On
	RewriteRule ^([^/.]+)/([^/.]+)/([^/.]+)$ index.php?study-name=$1&site-number=$2&page-name=$3
	RewriteCond %{HTTPS} off [OR]
    RewriteCond %{HTTP_HOST} ^www\. [NC]
    RewriteCond %{HTTP_HOST} ^(?:www\.)?(.+)$ [NC]
    RewriteRule ^ https://%1%{REQUEST_URI} [L,NE,R=301]
</IfModule>